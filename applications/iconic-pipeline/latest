proc ModulesHelp { } {
	puts stderr "This module adds the ICONIC pipeline scripts package to your environment."
        puts stderr ""
        }

module-whatis "Adds ICONIC pipeline scripts to your environment."

# Add scripts to PYTHONPATH
prepend-path PYTHONPATH /shared/ucl/apps/pipeline/pipeline
prepend-path PATH /shared/ucl/apps/pipeline/pipeline

